{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "csa/guzzle-bundle": "^3.0",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "jms/serializer-bundle": "^2.3",
        "knpuniversity/oauth2-client-bundle": "^1.15",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^2.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/console": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/workflow": "^3.4",
        "symfony/yaml": "^3.4",
        "symfony/form": "^3.4",
        "symfony/security": "^3.4",
        "symfony/inflector": "^3.4",
        "symfony/property-access": "^3.4"
    },
    "require-dev": {
        "alexandresalome/mailcatcher": "^1.2",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^2.7",
        "friendsofphp/php-cs-fixer": "^2.8",
        "knplabs/friendly-contexts": "^0.8.2",
        "phploc/phploc": "^4.0",
        "phpmetrics/phpmetrics": "^2.3",
        "phpstan/phpstan": "^0.9.1",
        "phpunit/phpunit": "^6.5",
        "sensiolabs/security-checker": "^4.1",
        "symfony/debug-bundle": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0DVJBJBFYBHGSXR6FK17T60",
            "allow-contrib": false
        }
    }
}
